{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
      "beforeBuildCommand": "yarn build",
      "beforeDevCommand": "yarn dev",
      "devPath": "http://localhost:5173",
      "distDir": "../dist"
  },
  "package": {
      "productName": "YTVIEWS",
      "version": "0.1.6"
  },
  "tauri": {
      "allowlist": {
          "all": true,
          "fs": {
              "all": true,
              "scope": ["**"]
          }
      },
      "bundle": {
          "active": true,
          "category": "Productivity",
          "copyright": "Copyright @ OLDI-K, OOO 2022, All rights reserved",
          "deb": {
              "depends": []
          },
          "externalBin": [],
          "icon": [
              "icons/32x32.png",
              "icons/128x128.png",
              "icons/128x128@2x.png",
              "icons/icon.icns",
              "icons/icon.ico",
              "icons/icon.png"
          ],
          "identifier": "dev.kmoz.YTVIEWS-alpha",
          "longDescription": "youtube bot dashbord",
          "macOS": {
              "license": "./LICENCE",
              "entitlements": "./entitlements.plist",
              "exceptionDomain": "kmoz.dev",
              "minimumSystemVersion": "10.7",
              "frameworks": [],
              "providerShortName": "Artem Semenov",
              "signingIdentity": "Developer ID Application: Artem Semenov (MVY72CHU7M)"  
          },
          "shortDescription": "youtube bot dashbord",
          "targets": "all",
          "windows": {
              "certificateThumbprint": null,
              "digestAlgorithm": "sha256",
              "timestampUrl": null
          }
      },
      "security": {
          "csp": null
      },
      "updater": {
          "active": false
      },
      "windows": [
          {
              "fullscreen": false,
              "height": 700,
              "resizable": false,
              "title": "YTVIEWS",
              "width": 900,
              "transparent": true,
              "decorations": true,
              "theme": "Light",
              "fileDropEnabled": false
          }
      ],
      "macOSPrivateApi": true
  }
}